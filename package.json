  "description": "Get domain from an url",
  "main": "index.js",
  "metapak": {
    "configs": [
      "readme"
    ],
    "data": {
      "testsFiles": "{,!(node_modules)/**/}*.spec.js",
      "files": "{,!(node_modules)/**/}*.js"
    }
  },
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
