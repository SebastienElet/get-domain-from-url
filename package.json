  },
  "homepage": "https://github.com/SebastienElet/get-domain-from-url#readme",
  "devDependencies": {
    "conventional-changelog-cli": "1.3.1",
    "cz-conventional-changelog": "2.0.0",
    "eslint": "3.19.0",
    "eslint-config-simplifield": "5.0.1",
    "jest": "19.0.2",
    "metapak": "^0.0.18",
    "metapak-sebastienelet": "*"
  },
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "ramda": "0.23.0"
  }
